{
    "name": "vue-filters-collection",
    "version": "0.2.1",
    "private": false,
    "description": "A collection of handy vuejs filters.",
    "author": "myolisi <myolisi@ngubentombi.co.za>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Myolisi/vue-filter-collection.git"
